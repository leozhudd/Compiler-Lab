    br label %r8
r8:
    %r11 = load i32, i32* %r5
    %r12 = sub i32 0, 1
    %r13 = icmp sgt i32 %r11, %r12
    %r14 = zext i1 %r13 to i32
    %r15 = icmp ne i32 %r14, 0
