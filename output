declare void @memset(i32*, i32, i32)
declare i32 @getint()
declare void @putint(i32)
declare i32 @getch()
declare void @putch(i32)
declare i32 @getarray(i32*)
declare void @putarray(i32, i32*)

define dso_local i32 @main() {
    %r1 = alloca i32
    store i32 1, i32* %r1
    %r2 = alloca i32
    store i32 2, i32* %r2
    %r3 = alloca i32
    store i32 3, i32* %r3
    %r7 = load i32, i32* %r1
    %r8 = sub i32 %r7, 1
    %r9 = icmp ne i32 %r8, 0
    br i1 %r9, label %r6, label %r5

r6:
    %r10 = load i32, i32* %r2
    %r11 = sub i32 %r10, 2
    %r13 = icmp ne i32 %r11, 0
    br i1 %r13, label %r4, label %r5
r4:
    %r14 = load i32, i32* %r1
    call void @putint(i32 %r14)
    br label %r12
r5:
    br label %r12
r12:
    %r15 = load i32, i32* %r2
    call void @putint(i32 %r15)
ret i32 0
}
