declare i32 @getint()
declare void @putint(i32)
declare i32 @getch()
declare void @putch(i32)
define dso_local i32 @main(){
    %1 = alloca i32
    store i32 10, i32* %1
    %2 = load i32, i32* %1
    %3 = icmp eq i32 %2, 0
    %4 = zext i1 %3 to i32
    %5 = icmp sgt i32 %4, 1
    br i1 %5, label %6, label %7
6:
    ret i32 1
    br label %8
7:
    br label %8
8:
    ret i32 0
}
