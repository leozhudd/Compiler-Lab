    %r15 = srem i32 %r1, %r2
    %r16 = getelementptr i32, i32* %r3, i32 0
    %r17 = getelementptr i32, i32* %r4, i32 0
    %r14 = call i32 @exgcd(i32 %r2, i32 %r15, i32* %r16, i32* %r17)
